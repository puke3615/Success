<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.success"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:name="com.example.success.base.PKApplication"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.success.view.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.success.view.NoteListActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.success.view.NoteDetailActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
